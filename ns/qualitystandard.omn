Prefix: agegroup: <http://ld.nice.org.uk/ns/qualitystandard/agegroup#>
Prefix: conditiondisease: <http://ld.nice.org.uk/ns/qualitystandard/conditiondisease#>
Ontology: <http://ld.nice.org.uk/ns/qualitystandard>

AnnotationProperty: <http://ld.nice.org.uk/ns/qualitystandard#include_drug_abuse,_alcohol_abuse,_obesity>

    SubPropertyOf: 
        rdfs:comment
    
    
AnnotationProperty: rdfs:comment

    
AnnotationProperty: skos:altLabel

    
Datatype: rdfs:Literal

    
Datatype: xsd:string

    
Datatype: rdf:PlainLiteral

    
ObjectProperty: condition

    Domain: pico:Population
    
    Range: 
        conditiondisease:ConditionDisease
    
    
ObjectProperty: <http://ld.nice.org.uk/ns/qualitystandard#prov:generated>

    Domain: 
        QualityStandard
    
    Range: 
        QualityStatement
    
    InverseOf: 
        <http://ld.nice.org.uk/ns/qualitystandard#prov:wasGeneratedby>
    
    
ObjectProperty: <http://ld.nice.org.uk/ns/qualitystandard#prov:wasGeneratedby>

    Domain: 
        QualityStatement
    
    Range: 
        QualityStandard
    
    InverseOf: 
        <http://ld.nice.org.uk/ns/qualitystandard#prov:generated>
    
    
ObjectProperty: measuresProgressAgainst

    Domain: 
        QualityMeasure
    
    
ObjectProperty: hasConcept

    InverseOf: 
        skos:inConceptScheme
    
    
ObjectProperty: definesTask

    Annotations: 
        rdfs:comment "connects  proposed action of quality statement to tasks which enable this action to take place"
    
    Domain: 
        QualityStatement
    
    SubPropertyOf: 
        owl:topObjectProperty
    
    InverseOf: 
        definedTaskOf
    
    
ObjectProperty: isServiceAreaOf

    Domain: 
        servicearea:ServiceArea
    
    Range: 
        QualityStatement
    
    InverseOf: 
        serviceArea
    
    
ObjectProperty: owl:topObjectProperty

    
ObjectProperty: definesMeasure

    Annotations: 
        rdfs:comment "one quality statement may hve several measures associated with it."
    
    Domain: 
        QualityStatement
    
    Range: 
        QualityMeasure
    
    InverseOf: 
        measureDefinedBy
    
    
ObjectProperty: isTargetPopulationOf

    Domain: 
        pico:Population
    
    Range: 
        QualityStatement
    
    SubPropertyOf: 
        owl:topObjectProperty
    
    InverseOf: 
        targetPopulation
    
    
ObjectProperty: age

    Domain: 
        pico:Population
    
    Range: 
        agegroup:AgeGroup
    
    
ObjectProperty: targetPopulation

    Domain: 
        QualityStatement
    
    Range: 
        pico:Population
    
    InverseOf: 
        isTargetPopulationOf
    
    
ObjectProperty: skos:inConceptScheme

    InverseOf: 
        hasConcept
    
    
ObjectProperty: ownsTask

    InverseOf: 
        taskOwner
    
    
ObjectProperty: serviceArea

    Domain: 
        QualityStatement
    
    Range: 
        servicearea:ServiceArea
    
    InverseOf: 
        isServiceAreaOf
    
    
ObjectProperty: specifies

    Domain: 
        PopulationSpecifier
    
    Range: 
        pico:Population
    
    InverseOf: 
        isSpecifiedBy
    
    
ObjectProperty: lifestylecondition:lifestyleCondition

    Domain: 
        pico:Population
    
    Range: 
        lifestylecondition:LifestyleCondition
    
    
ObjectProperty: isSettingOf

    Domain: 
        setting:Setting
    
    Range: 
        QualityStatement
    
    InverseOf: 
        setting
    
    
ObjectProperty: measureDefinedBy

    Domain: 
        QualityMeasure
    
    Range: 
        QualityStatement
    
    InverseOf: 
        definesMeasure
    
    
ObjectProperty: isSpecifiedBy

    Domain: 
        pico:Population
    
    Range: 
        PopulationSpecifier
    
    InverseOf: 
        specifies
    
    
ObjectProperty: setting

    Domain: 
        QualityStatement
    
    Range: 
        setting:Setting
    
    InverseOf: 
        isSettingOf
    
    
ObjectProperty: definedTaskOf

    Range: 
        QualityStatement
    
    InverseOf: 
        definesTask
    
    
ObjectProperty: taskOwner

    InverseOf: 
        ownsTask
    
    
DataProperty: hasDataSource

    Domain: 
        QualityMeasure
    
    Range: 
        rdfs:Literal
    
    
DataProperty: dc:title

    Domain: 
        QualityStandard
    
    Range: 
        rdfs:Literal
    
    
DataProperty: dc:identifier

    Domain: 
        owl:Thing
    
    Range: 
        rdfs:Literal
    
    
DataProperty: <http://ld.nice.org.uk/ns/qualitystandard#qs::hasNumerator>

    Domain: 
        ProcessMeasure
    
    Range: 
        xsd:string
    
    
DataProperty: <http://ld.nice.org.uk/ns/qualitystandard#qs::hasDenominator>

    Domain: 
        QualityMeasure
    
    Range: 
        xsd:string
    
    
Class: lifestylecondition:LifestyleCondition

    Annotations: 
        rdfs:comment "to include drug abuse, alcohol abuse and other lifestyle conditions"
    
    SubClassOf: 
        PopulationSpecifier
    
    
Class: pico:Population

    Annotations: 
        rdfs:comment "defined by at least one of the age, gender, ethnicity, disease, lifestyle condition concepts."
    
    EquivalentTo: 
        isSpecifiedBy min 1 PopulationSpecifier
    
    
Class: agegroup:AgeGroup

    Annotations: 
        rdfs:comment "Some list of age groups to be defined to support population definition"
    
    SubClassOf: 
        PopulationSpecifier
    
    
Class: QualityMeasure

    Annotations: 
        rdfs:comment "measure associated with quality statement"
    
    DisjointWith: 
        QualityStandard, QualityStatement
    
    
Class: QualityStatement

    SubClassOf: 
        setting some setting:Setting,
        targetPopulation some PopulationSpecifier,
        serviceArea some servicearea:ServiceArea
    
    DisjointWith: 
        QualityStandard, QualityMeasure
    
    
Class: ProcessMeasure

    SubClassOf: 
        QualityMeasure
    
    DisjointWith: 
        OutcomeMeasure, StructureMeasure
    
    
Class: owl:Thing

    
Class: servicearea:ServiceArea

    Annotations: 
        rdfs:comment "Service area for which quality statement is relevant."
    
    
Class: conditiondisease:ConditionDisease

    Annotations: 
        rdfs:comment "Base on NICE Browse taxonomy structure with mapping to equivalent SNOMED code where possible",
        rdfs:comment "use for medical condition"
    
    SubClassOf: 
        PopulationSpecifier
    
    
Class: QualityStandard

    DisjointWith: 
        QualityStatement, QualityMeasure
    
    
Class: StructureMeasure

    SubClassOf: 
        QualityMeasure
    
    DisjointWith: 
        ProcessMeasure, OutcomeMeasure
    
    
Class: setting:Setting

    Annotations: 
        rdfs:comment "Setting - may be used for setting for population definition as well as setting where action of quality statement should occur"
    
    
Class: PopulationSpecifier

    
Class: OutcomeMeasure

    SubClassOf: 
        QualityMeasure
    
    DisjointWith: 
        ProcessMeasure, StructureMeasure
    
    
