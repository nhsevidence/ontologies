    DisjointWith: 
        <http://ld.nice.org.uk/ns/qualitystandard#OutcomeMeasure>, <http://ld.nice.org.uk/ns/qualitystandard#StructureMeasure>
    
    
Class: <http://ld.nice.org.uk/ns/qualitystandard#Product>

    
Class: <http://ld.nice.org.uk/ns/qualitystandard#Published>

    Annotations: 
        rdfs:comment "Status of a resource that has been published and has not been withdrawn (still valid)."
    
    SubClassOf: 
        <http://ld.nice.org.uk/ns/qualitystandard#GuidanceStatus>,
        <http://ld.nice.org.uk/ns/qualitystandard#dateIssued> some xsd:dateTime,
        <http://ld.nice.org.uk/ns/qualitystandard#dateWithdrawn> max 0 xsd:dateTime
    
    DisjointWith: 
        <http://ld.nice.org.uk/ns/qualitystandard#Withdrawn>
    
    
Class: <http://ld.nice.org.uk/ns/qualitystandard#QualityMeasure>

    Annotations: 
        rdfs:comment "Quality measures are used to assess the quality of care or service provision specified in the quality statement."
    
    
Class: <http://ld.nice.org.uk/ns/qualitystandard#QualityStandard>

    Annotations: 
        rdfs:comment "NICE quality standards are concise sets of prioritised  quality statements designed to drive measurable quality improvements within a particular area of health or care."
    
    SubClassOf: 
        <http://ld.nice.org.uk/ns/qualitystandard#Product>,
        <http://ld.nice.org.uk/ns/qualitystandard#hasPart> some <http://ld.nice.org.uk/ns/qualitystandard#QualityStatement>
    
    DisjointWith: 
        <http://ld.nice.org.uk/ns/qualitystandard#QualityStatement>
    
    
Class: <http://ld.nice.org.uk/ns/qualitystandard#QualityStatement>

    Annotations: 
        rdfs:comment "A NICE Quality statement is a clear, measurable and concise statement to describe ‘enhanced practice’, which is both aspirational and achievable."
    
    SubClassOf: 
        <http://ld.nice.org.uk/ns/qualitystandard#Product>,
        <http://ld.nice.org.uk/ns/qualitystandard#partOf> some <http://ld.nice.org.uk/ns/qualitystandard#QualityStandard>
    
    DisjointWith: 
        <http://ld.nice.org.uk/ns/qualitystandard#QualityStandard>
    
    
Class: <http://ld.nice.org.uk/ns/qualitystandard#StructureMeasure>

    Annotations: 
        rdfs:comment "A  structure measure addresses the structure of care or services."
    
    SubClassOf: 
        <http://ld.nice.org.uk/ns/qualitystandard#QualityMeasure>
    
    DisjointWith: 
        <http://ld.nice.org.uk/ns/qualitystandard#OutcomeMeasure>, <http://ld.nice.org.uk/ns/qualitystandard#ProcessMeasure>
    
    
Class: <http://ld.nice.org.uk/ns/qualitystandard#TopLevelProvenanceConcept>

    
Class: <http://ld.nice.org.uk/ns/qualitystandard#Withdrawn>

    Annotations: 
        rdfs:comment "Status of a resource that thas been withdrawn after some time when it was a valid, published resource. No longer valid."
    
    SubClassOf: 
        <http://ld.nice.org.uk/ns/qualitystandard#GuidanceStatus>,
        <http://ld.nice.org.uk/ns/qualitystandard#dateWithdrawn> some xsd:dateTime
    
    DisjointWith: 
        <http://ld.nice.org.uk/ns/qualitystandard#Published>
    
    
